1.factor of given all numbers 
import java.util.Scanner;
public class Main
{
   public static void main (String[] args) {
       Scanner sc=new Scanner(System.in);
       int num=sc.nextInt();
       int count;
       for(int i=1;i<=num;i++)
       {
           if((num%i)==0){
               System.out.print(i+" ");
           }
           
           
       }
    }
}

2.Factorial of given number
import java.util.Scanner;
public class Main
{
   public static void main (String[] args) {
       Scanner sc=new Scanner(System.in);
       int num=sc.nextInt();
       int fact;
      for(fact=1;num>=1;num--)
           fact=fact*num;
       System.out.print(fact);
       
        
    }
}
